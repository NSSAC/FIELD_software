function risk(){ # schematic 
    python ../scripts/county_dist.py -s colorado -c weld -l cattle -m farms -o weld_farms.pdf
    python ../scripts/county_dist.py -s colorado -c weld -l cattle -m birds -o weld_birds.pdf 
    python ../scripts/county_dist.py -s colorado -c weld -l cattle -m risk -o weld_risk.pdf
}

function verify(){ # Verification of counts
    python ../scripts/verify.py
}

function schcountry(){ # plots for schematic
    python ../scripts/livestock_3d.py -m livestock -s cattle -o us_cattle
    python ../scripts/livestock_3d.py -m livestock -s hogs -o us_hogs
    python ../scripts/livestock_3d.py -m livestock -s chickens -o us_chickens
}

