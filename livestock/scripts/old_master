function pop(){
    python ../scripts/pop_3d.py -o us_pop
    python ../scripts/pop_3d.py --admin state --state virginia -o va_pop -l ../../data/processing_centers/processing_centers.csv.zip
}

function transfer(){
    cp birds.png ../../../livestock_digital_twin/journal/figs/
    mv birds.png ../results/
    mv farms_to_cells.csv.zip ../results/
    mv stats_farms_to_cells.csv.zip ../results/
    cp farms_to_cells_dist.pdf ../../../livestock_digital_twin/journal/figs/
    mv farms_to_cells_dist.pdf ../results/
    cp stats_farms_to_cells.pdf ../../../livestock_digital_twin/journal/figs/
    mv stats_farms_to_cells.pdf ../results/
    cp farms_to_cells_dist*_texas.pdf ../../../livestock_digital_twin/journal/figs/
    mv farms_to_cells_dist*_texas.pdf ../results/
    cp farms_to_cells_dist*_virginia.pdf ../../../livestock_digital_twin/journal/figs/
    mv farms_to_cells_dist*_virginia.pdf ../results/
    cp farms_to_cells_dist*_washington.pdf ../../../livestock_digital_twin/journal/figs/
    mv farms_to_cells_dist*_washington.pdf ../results/
    cp farms_to_cells_dist*_california.pdf ../../../livestock_digital_twin/journal/figs/
    mv farms_to_cells_dist*_california.pdf ../results/
    cp farms_to_cells_dist*_iowa.pdf ../../../livestock_digital_twin/journal/figs/
    mv farms_to_cells_dist*_iowa.pdf ../results/
    cp farms_to_cells_dist*_ohio.pdf ../../../livestock_digital_twin/journal/figs/
    mv farms_to_cells_dist*_ohio.pdf ../results/
    cp cafo_analysis.pdf ../../../livestock_digital_twin/journal/figs/
    mv cafo_analysis.pdf ../results/
}


